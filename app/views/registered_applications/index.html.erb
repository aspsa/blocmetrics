<h1>Registered Applications</h1>
<h4><%= link_to "Register an Appplication", new_registered_application_path %></h4>
<br />

<% @reg_apps.each do |ra| %>
  <p>
      <%= ra.name %> : <%= ra.url %> | 
      <%= link_to "Edit", edit_registered_application_path(ra) %> | 
      <%= link_to "Delete", registered_application_path(ra), data: { confirm: "Are you sure?" }, method: :delete %>
  </p>
<% end %>